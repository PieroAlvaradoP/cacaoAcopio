{% extends 'base.html.twig' %}

{% block title %}Socio{% endblock %}

{% block body %}
    <h1>Socio</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ socio.id }}</td>
            </tr>
            <tr>
                <th>CodigoSocio</th>
                <td>{{ socio.codigoSocio }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ socio.createdAt ? socio.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ socio.updatedAt ? socio.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{{ socio.activo ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Uuid</th>
                <td>{{ socio.uuid }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('socio_index') }}">back to list</a>

    <a href="{{ path('socio_edit', {'id': socio.id}) }}">edit</a>

    {{ include('socio/_delete_form.html.twig') }}
{% endblock %}
