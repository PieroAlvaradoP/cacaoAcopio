{% extends 'base.html.twig' %}
{% import 'macro/breadcrumb.html.twig' as breadcrumb %}
{% import 'macro/action.html.twig' as action %}

{% set _title = 'Base Social' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'base_social_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ action.tbl_start() }}
    {{ action.tbl_tr('Nombre',base_social.nombre) }}
    {{ action.tbl_tr('Localidad',base_social.localidad) }}
    {{ action.tbl_tr('Almacen',base_social.almacenAcopio) }}
    {% include 'crud/_show_info.html.twig' with {'entity': base_social} %}
    {{ action.tbl_end() }}

{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'base_social','entity': base_social} %}
{% endblock %}

{% block _main_tools %}
    {% include 'crud/_tools_ico.html.twig' %}
{% endblock %}

{% block _body_tools %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'base_social','entity': base_social} %}
{% endblock %}

{% block _body_tools_ico %}
    {% include 'crud/_show_links_tool.html.twig' with {'route_base':'base_social','entity': base_social} %}
{% endblock %}