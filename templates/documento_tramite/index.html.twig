{% extends 'base.html.twig' %}

{% block title %}DocumentoTramite index{% endblock %}

{% block body %}
    <h1>DocumentoTramite index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Serie</th>
                <th>Numero</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for documento_tramite in documento_tramites %}
            <tr>
                <td>{{ documento_tramite.id }}</td>
                <td>{{ documento_tramite.serie }}</td>
                <td>{{ documento_tramite.numero }}</td>
                <td>
                    <a href="{{ path('app_documento_tramite_show', {'id': documento_tramite.id}) }}">show</a>
                    <a href="{{ path('app_documento_tramite_edit', {'id': documento_tramite.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_documento_tramite_new') }}">Create new</a>
{% endblock %}
