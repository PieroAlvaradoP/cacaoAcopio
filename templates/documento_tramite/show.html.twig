{% extends 'base.html.twig' %}
{% import 'macro/breadcrumb.html.twig' as breadcrumb %}
{% import 'macro/action.html.twig' as action %}

{% set _title = 'Documento Tramite' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'documento_tramite_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ action.tbl_start() }}
    {{ action.tbl_tr('Nombre',documento_tramite.tipoDocumentoTramite) }}
    {{ action.tbl_tr('RUC',documento_tramite.serie) }}
    {{ action.tbl_tr('Direcci√≥n',documento_tramite.numero) }}
    {% include 'crud/_show_info.html.twig' with {'entity': documento_tramite} %}
    {{ action.tbl_end() }}

{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'documento_tramite','entity': documento_tramite} %}
{% endblock %}

{% block _main_tools %}
    {% include 'crud/_tools_ico.html.twig' %}
{% endblock %}

{% block _body_tools %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'documento_tramite','entity': documento_tramite} %}
{% endblock %}

{% block _body_tools_ico %}
