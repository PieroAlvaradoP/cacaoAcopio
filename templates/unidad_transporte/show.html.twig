{% extends 'base.html.twig' %}
{% import 'macro/breadcrumb.html.twig' as breadcrumb %}
{% import 'macro/action.html.twig' as action %}

{% set _title = 'Unidad de Transporte' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'unidad_transporte_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ action.tbl_start() }}
    {{ action.tbl_tr('Empresa de Transporte',unidadTransporte.empresaTransporte) }}
    {{ action.tbl_tr('Tipo de Vehículo',unidadTransporte.tipoVehiculo) }}
    {{ action.tbl_tr('Número de Ejes',unidadTransporte.numEjes) }}
    {% include 'crud/_show_info.html.twig' with {'entity': unidadTransporte} %}
    {{ action.tbl_end() }}

{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'unidad_transporte','entity': unidadTransporte} %}
{% endblock %}

{% block _main_tools %}
    {% include 'crud/_tools_ico.html.twig' %}
{% endblock %}

{% block _body_tools %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'unidad_transporte','entity': unidadTransporte} %}
{% endblock %}

{% block _body_tools_ico %}
    {% include 'crud/_show_links_tool.html.twig' with {'route_base':'unidad_transporte','entity': unidadTransporte} %}
{% endblock %}