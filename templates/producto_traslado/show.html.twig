{% extends 'base.html.twig' %}

{% block title %}ProductoTraslado{% endblock %}

{% block body %}
    <h1>ProductoTraslado</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ producto_traslado.id }}</td>
            </tr>
            <tr>
                <th>CantSacos</th>
                <td>{{ producto_traslado.cantSacos }}</td>
            </tr>
            <tr>
                <th>PesoBruto</th>
                <td>{{ producto_traslado.pesoBruto }}</td>
            </tr>
