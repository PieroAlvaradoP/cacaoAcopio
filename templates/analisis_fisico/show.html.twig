{% extends 'base.html.twig' %}
{% import 'macro/breadcrumb.html.twig' as breadcrumb %}
{% import 'macro/action.html.twig' as action %}

{% set _title = 'Analisis Fisico' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'analisis_fisico_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ action.tbl_start() }}
    {{ action.tbl_tr('Nombre',analisis_fisico.periodo) }}
    {{ action.tbl_tr('Fecha',analisis_fisico.fecha) }}
    {{ action.tbl_tr('Ticket',analisis_fisico.ticket) }}
    {{ action.tbl_tr('Muestra',analisis_fisico.muestra) }}
    {{ action.tbl_tr('Exportable (gr)',analisis_fisico.exportable) }}
    {{ action.tbl_tr('Exportable (%)',analisis_fisico.exportablePorcentaje) }}
    {{ action.tbl_tr('Humedad',analisis_fisico.humedad) }}
    {{ action.tbl_tr('Activo',analisis_fisico.activo | yesnocustom) }}
    {{ action.tbl_end() }}
{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'analisis_fisico','entity': analisis_fisico} %}
{% endblock %}

{% block _main_tools %}
    {% include 'crud/_tools_ico.html.twig' %}
{% endblock %}

{% block _body_tools %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'analisis_fisico','entity': analisis_fisico} %}
{% endblock %}

{% block _body_tools_ico %}
    {% include 'crud/_show_links_tool.html.twig' with {'route_base':'analisis_fisico','entity': analisis_fisico} %}
{% endblock %}