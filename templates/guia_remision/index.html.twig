{% extends 'base.html.twig' %}

{% block title %}GuiaRemision index{% endblock %}

{% block body %}
    <h1>GuiaRemision index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>FechaTraslado</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for guia_remision in guia_remisions %}
            <tr>
                <td>{{ guia_remision.id }}</td>
                <td>{{ guia_remision.fechaTraslado ? guia_remision.fechaTraslado|date('Y-m-d') : '' }}</td>
                <td>
                    <a href="{{ path('app_guia_remision_show', {'id': guia_remision.id}) }}">show</a>
                    <a href="{{ path('app_guia_remision_edit', {'id': guia_remision.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_guia_remision_new') }}">Create new</a>
{% endblock %}
