{% extends 'base.html.twig' %}
{% import 'macro/breadcrumb.html.twig' as breadcrumb %}
{% import 'macro/action.html.twig' as action %}

{% set _title = 'Guia de Remisi√≥n' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'guia_remision_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ action.tbl_start() }}
    {{ action.tbl_tr('ID',guia_remision.id) }}
    {{ action.tbl_tr('Fecha de Traslado',guia_remision.fechaTraslado) }}
    {{ action.tbl_tr('Periodo',guia_remision.periodo) }}
    {{ action.tbl_tr('Almacen Origen',guia_remision.almacenOrigen) }}
    {{ action.tbl_tr('Almacen Destino',guia_remision.almacenDestino) }}
    {{ action.tbl_tr('Unidad de Transporte',guia_remision.unidadTransporte) }}
    {{ action.tbl_tr('Conductor',guia_remision.conductor) }}
    {{ action.tbl_tr('Motivo de Transporte',guia_remision.motivoTraslado) }}
    {{ action.tbl_tr('Productos',guia_remision.producto) }}
    {% include 'crud/_show_info.html.twig' with {'entity': guia_remision} %}
    {{ action.tbl_end() }}

{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'guia_remision','entity': guia_remision} %}
{% endblock %}

{% block _main_tools %}
    {% include 'crud/_tools_ico.html.twig' %}
{% endblock %}

{% block _body_tools %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'guia_remision','entity': guia_remision} %}
{% endblock %}

{% block _body_tools_ico %}
    {% include 'crud/_show_links_tool.html.twig' with {'route_base':'guia_remision','entity': guia_remision} %}
{% endblock %}